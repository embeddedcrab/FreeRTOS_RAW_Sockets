 264:../Startup/system_XMC4500.c **** /* Setup the System clock */ 
 265:../Startup/system_XMC4500.c **** #if SCU_CLOCK_SETUP
 266:../Startup/system_XMC4500.c **** SystemClockSetup();
 267:../Startup/system_XMC4500.c **** #endif
